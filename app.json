    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.markrabey.water-fest-demo"
    },
    "android": {
      "package": "com.markrabey.water_fest_demo",
      "permissions": []
    }
  }
}
